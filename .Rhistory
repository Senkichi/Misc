extcdf = function(mu,sig2,d,tmax=50)
{
G = numeric(tmax)
x=1:tmax
G = pnorm((-d-mu*x)/sqrt(sig2*x)) +
exp(-2*mu*d/sig2)* pnorm((-d+mu*x)/sqrt(sig2*x))
}
